{
  "cSpell.language": "en,pt",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json",
    "*.tsx": "typescriptreact",
    ".eslintrc.json": "jsonc"
  },
  "editor.rulers": [
    80,
    120
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "index.js": true,
    "index.d.ts": true,
    "components/**": true,
    "containers/**": true,
    "contexts/**": true,
    "effects/**": true,
    "hooks/**": true,
    "settings/**": true,
    "styled/**": true,
    "types/**": true,
    "utilities/**": true
  }
}
